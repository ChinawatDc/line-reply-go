{
    "version": 2,
    "builds": [
        {
            "src": "main.go",
            "use": "@vercel/go"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "/main.go"
        },
        {
            "src": "/reply",
            "dest": "/main.go"
        }
    ]
}